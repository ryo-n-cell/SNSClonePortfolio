{
  "connection": "default",
  "collectionName": "accounts_authentications",
  "info": {
    "name": "accounts_ authentication"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "login_id": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "string"
    },
    "join_on": {
      "type": "datetime"
    },
    "user_statuses": {
      "collection": "user-status",
      "via": "accounts_authentications",
      "dominant": true
    },
    "all_tweets": {
      "via": "accounts_authentication",
      "collection": "all-tweet"
    },
    "follow_statuses_follower_id": {
      "collection": "follow-status",
      "via": "accounts_authentication"
    },
    "user_relations": {
      "collection": "user-relation",
      "via": "accounts_authentication"
    },
    "user_id": {
      "type": "integer"
    },
    "follow_statuses_follow_id": {
      "collection": "follow-status",
      "via": "accounts_authentication"
    }
  }
}
