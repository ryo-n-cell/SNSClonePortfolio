{
  "connection": "default",
  "collectionName": "follows",
  "info": {
    "name": "Accounts_ authentication"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "User_id": {
      "type": "integer",
      "unique": true,
      "required": true
    },
    "login_id": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "Join_on": {
      "type": "date"
    },
    "all_tweet": {
      "model": "personal-tweet",
      "via": "accounts_authentication"
    }
  }
}
